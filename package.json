{
  "name": "next-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:order-imports": "prettier . --write",
    "lint:biome": "biome check .",
    "lint:biome:fix": "biome check . --write",
    "format:biome": "biome format .",
    "format:biome:fix": "biome format . --write",
    "check:biome": "npm run lint:biome && npm run format:biome",
    "fix:biome": "npm run lint:biome:fix && npm run format:biome:fix",
    "fix:biome:prettier": "npm run prettier:order-imports && npm run lint:biome:fix && npm run format:biome:fix"
  },
  "dependencies": {
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@eslint/eslintrc": "^3",
    "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
